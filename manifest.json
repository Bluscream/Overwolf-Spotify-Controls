{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"Spotify Desktop Controls",
    "version":"1.3.1.1",
    "minimum-overwolf-version": "0.77.4.0",
    "author":"GamingVibe, Bluscream",
    "icon":"IconMouseOver.png",
    "icon_gray":"IconMouseNormal.png",
    "description":"Controls the Spotify Desktop application."
  },
  "data": {
    "start_window":"index",
    "windows":{
      "index":{
        "file":"Files/index.html",
        "transparent":true,
        "resizable":false,
        "show_in_taskbar": false,
        "size": {
          "width": 430,
          "height": 250
        },
		"min_size":{
		  "width": 430,
          "height": 250
		},
		"max_size":{
		  "width": 430,
          "height": 250
		},
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    },
    "plugins": [
      "Files/GVMediaInterfacePlugin.dll"
    ]
  }
}